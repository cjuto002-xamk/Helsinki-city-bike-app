// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Journey {
  id            Int    @id @default(autoincrement())
  departure     Int
  return        Int
  dsid          Int
  dsname        String
  rsid          Int
  rsname        String
  distance      Int
  duration      Int
}
